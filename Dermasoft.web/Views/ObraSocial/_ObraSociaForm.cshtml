@model Dermasoft.web.Models.ObraSocialModel
<div class="row">
    <div class="col l6 offset-l3 m12 s12">
        <div class="card">
            <div class="card-content">
                <h4 class="center-align"> @ViewBag.Action Obra Social </h4>
                @using (Html.BeginForm("ObraSocialSave", "ObraSocial"))
                {
                    @Html.HiddenFor(model => model.IdObraSocial)

                    @Html.LabelFor(model => model.NombreObraSocial)
                    @Html.TextBoxFor(model => model.NombreObraSocial)
                    @Html.ValidationMessageFor(model => model.NombreObraSocial)

                    @Html.LabelFor(model => model.Clave)
                    @Html.TextBoxFor(model => model.Clave)
                    @Html.ValidationMessageFor(model => model.Clave)

                    <div class="space-v-lg"></div>

                    <a href="@Url.Action("Index", "ObraSocial")" class="btn btn-floating btn-large waves-effect waves-light red"><i class="mdi-content-clear"></i></a>
                    <button type="submit" class="btn btn-floating btn-large waves-effect waves-light green right"><i class="mdi-action-done"></i> </button>
                }
            </div>
        </div>
    </div>
</div>